{
  "name": "teamgantt-split-test",
  "version": "0.1.0",
  "description": "A library to randomly put a user in view the original version (Control) or view an experimental version (Variation) of a test.",
  "main": "src/index.cjs",
  "type": "module",
  "scripts": {
    "build": "webpack --config webpack.config.cjs --mode production",
    "test": "node ./test/index.test.mjs",
    "prettier": "prettier --config .prettierrc.cjs --write \"{src,test}/**/*.{cjs,mjs}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamgantt/split-test.git"
  },
  "keywords": [
    "split",
    "test"
  ],
  "author": "TeamGantt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teamgantt/split-test/issues"
  },
  "homepage": "https://github.com/teamgantt/split-test#readme",
  "devDependencies": {
    "prettier": "2.8.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^5.0.1"
  }
}
